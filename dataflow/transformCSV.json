{
	"name": "transformCSV",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dataflowsource",
						"type": "DatasetReference"
					},
					"name": "sourceCSV"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dataflowsync",
						"type": "DatasetReference"
					},
					"name": "sink"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "filterrow"
				},
				{
					"name": "split"
				},
				{
					"name": "derivedColumn"
				},
				{
					"name": "aggregate"
				},
				{
					"name": "alterRow"
				}
			],
			"scriptLines": [
				"source(output(",
				"          transaction_id as short,",
				"          transactional_date as timestamp,",
				"          product_id as string,",
				"          customer_id as short,",
				"          payment as string,",
				"          credit_card as long,",
				"          loyalty_card as boolean,",
				"          cost as double,",
				"          quantity as short,",
				"          price as double,",
				"          {_c10} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceCSV",
				"sourceCSV select(mapColumn(",
				"          transaction_id,",
				"          transactional_date,",
				"          product_id,",
				"          customer_id,",
				"          payment,",
				"          price,",
				"          {_c10}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 filter(customer_id !=12) ~> filterrow",
				"filterrow split(payment =='visa',",
				"     payment == 'mastercard',",
				"     disjoint: false) ~> split@(visa, mastercard, otherThanVisa)",
				"alterRow derive(payment = coalesce(payment,'N/A')) ~> derivedColumn",
				"split@mastercard aggregate(groupBy(customer_id),",
				"     product_id = max(product_id)) ~> aggregate",
				"split@otherThanVisa alterRow(insertIf(1==1)) ~> alterRow",
				"split@visa sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink"
			]
		}
	}
}